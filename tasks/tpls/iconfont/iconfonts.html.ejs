<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Icon Font</title>
  <style>
    body {

    }

    .icon-item {
      text-align: center;
      float: left;
      width: 20%;
    }

    .icon-item__icon {
      font-size: 144px;
      display: block;
    }

    .icon-item__icon > i {
      background-color: #eee;
      background-image: url('data:image/svg+xml;base64,');
      background-size: contain;
      background-repeat: no-repeat;
    }

    .icon-item__text {
      font-size: 18px;
      display: block;
      margin-bottom: 1em;
    }


    @font-face {
      font-family:"<%= fontConfig.fontName %>";
      src:url("../fonts/<%= fontConfig.fontName %>.eot?hash=<%= fontConfig.hash %>");
      src:url("../fonts/<%= fontConfig.fontName %>.eot?hash=<%= fontConfig.hash %>#iefix") format("embedded-opentype"),
      url("../fonts/<%= fontConfig.fontName %>.woff?hash=<%= fontConfig.hash %>") format("woff"),
      url("../fonts/<%= fontConfig.fontName %>.ttf?hash=<%= fontConfig.hash %>") format("truetype");
    }

    .<%= fontConfig.fontShortName %> {
      font-family:"<%= fontConfig.fontName %>";
      position: relative;
      top: 1px;
      display: inline-block;
      font-style: normal;
      font-weight: normal;
      line-height: 1;
      -webkit-font-smoothing: antialiased;
      -moz-osx-font-smoothing: grayscale;
    }

    <% for (var idx = 0; idx < codepoints.length; idx++) { %>
    .<%= fontConfig.fontShortName %>--<%= codepoints[idx].name %>:before { content: "\<%= codepoints[idx].codepoint %>";}<% } %>


  </style>
</head>
<body>

  <% for (var idx = 0; idx < codepoints.length; idx++) { %>
  <div class="icon-item">
    <span class="icon-item__icon">
      <i class="<%= fontConfig.fontShortName %> <%= fontConfig.fontShortName %>--<%= codepoints[idx].name %>"></i>
    </span>
    <span class="icon-item__text"><%= codepoints[idx].name %></span>
  </div>
  <% } %>


</body>
</html>
